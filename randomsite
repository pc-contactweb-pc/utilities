            ];


            window.onload = function () {
                randomizeSites();
            };



            function randomizeSites() {

                let outputAll = document.getElementById('randomAllp');
                let outputAllEn = document.getElementById("randomAllEn");
                let outputAllFr = document.getElementById("randomAllFr");

                let outputNP = document.getElementById('randomNPp');
                let outputNPEn = document.getElementById("randomNPEn");
                let outputNPFr = document.getElementById("randomNPFr");

                let outputNHS = document.getElementById('randomNHSp');
                let outputNHSEn = document.getElementById("randomNHSEn");
                let outputNHSFr = document.getElementById("randomNHSFr");

                let outputNMCA = document.getElementById('randomNMCAp');
                let outputNMCAEn = document.getElementById("randomNMCAEn");
                let outputNMCAFr = document.getElementById("randomNMCAFr");



                outputAll.innerHTML = "";
                outputAllEn.href = "";
                outputAllFr.href = "";

                outputNP.innerHTML = "";
                outputNPEn.href = "";
                outputNPFr.href = "";

                outputNHS.innerHTML = "";
                outputNHSEn.href = "";
                outputNHSFr.href = "";

                outputNMCA.innerHTML = "";
                outputNMCAEn.href = "";
                outputNMCAFr.href = "";


                // All



                let outputLocation = locations[Math.floor(Math.random() * locations.length)];
                outputAll.innerHTML = outputLocation.name;
                outputAllEn.href = outputLocation.path;
                outputAllFr.href = outputLocation.path.replace("/en/", "/fr/");


                // PN
                outputLocations = locations.filter(location => location.type === "pn-np");
                outputLocation = outputLocations[Math.floor(Math.random() * outputLocations.length)];
                outputNP.innerHTML = outputLocation.name;
                outputNPEn.href = outputLocation.path;
                outputNPFr.href = outputLocation.path.replace("/en/", "/fr/");


                // NHS


                outputLocations = locations.filter(location => location.type === "lhn-nhs");
                outputLocation = outputLocations[Math.floor(Math.random() * outputLocations.length)];
                outputNHS.innerHTML = outputLocation.name;
                outputNHSEn.href = outputLocation.path;
                outputNHSFr.href = outputLocation.path.replace("/en/", "/fr/");


                // NMCA

                outputLocations = locations.filter(location => location.type === "amnc-nmca");
                outputLocation = outputLocations[Math.floor(Math.random() * outputLocations.length)];
                outputNMCA.innerHTML = outputLocation.name;
                outputNMCAEn.href = outputLocation.path;
                outputNMCAFr.href = outputLocation.path.replace("/en/", "/fr/");


            }

        </script>
    </main> <!-- End main page content         -->
